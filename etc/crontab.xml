<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="forter_forter">
        <job name="send_queue" instance="Forter\Forter\Cron\SendQueue" method="execute">
            <schedule>* * * * *</schedule>
        </job>
        <job name="cancel_order" instance="Forter\Forter\Cron\CancelOrder" method="execute">
            <schedule>* * * * *</schedule>
        </job>
    </group>
</config>
